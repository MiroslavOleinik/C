int number;

int main()
{
	scanf("%i", &number);
	printf("Factorial is %d\n", factorial());
}

long long factorial()
{
	long long factorialAnswer = 1;
	int numberForFactorial = 1;
	for (int i = 0; i < number; i++)
	{
		factorialAnswer = factorialAnswer * numberForFactorial++;
	}
	return factorialAnswer;
}
